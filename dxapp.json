{
  "name": "eggd_dx_checker",
  "title": "dx_checker",
  "summary": "App to check for affects of weekly DNAnexus updates",
  "dxapi": "1.0.0",
  "properties": {
    "githubRelease": "v1.0.0"
  },
  "inputSpec": [
    {
      "name": "truth_vcf",
      "label": "original generated VCF",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "query_vcf",
      "label": "newly generated VCF",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "check_output",
      "label": "output of app",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 1
      }
    },
    "interpreter": "bash",
    "file": "src/dx_checker.sh",
    "distribution": "Ubuntu",
    "release": "16.04",
    "version": "1"
  },
  "access": {
    "network": [
      "*"
    ],
    "project": "CONTRIBUTE"
  },
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x4"
        }
      }
    }
  }
}
